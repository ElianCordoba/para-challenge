{
  "name": "activity-summarization",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all clean transpile && node ./build/bin/www.js",
    "transpile": "babel ./src --out-dir build --plugins source-map-support -s true",
    "clean": "rimraf build",
    "watch-start": "nodemon",
    "test": "node build/scripts/basicIntegrationTest.js"
  },
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "src/*",
      "public/*",
      "package.json"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "@seald-io/nedb": "^3.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "start": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "sourceMaps": true
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "assert": "^2.0.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "source-map-support": "^0.5.21"
  }
}
